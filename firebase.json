{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/pizza.de/**",
        "destination": "/pizza.de/index.html"
      },
      {
        "source": "/game-of-thrones/**",
        "destination": "/game-of-thrones/index.html"
      }
    ]
  }
}
